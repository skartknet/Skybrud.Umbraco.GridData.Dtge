<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45;net46;net47</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <Version>1.0.0-beta001</Version>
    <Authors>Anders Bjerner</Authors>
    <Description>DocTypeGridEditor addon for Skybrud.Umbraco.GridData</Description>
    <Copyright>Copyright © 2019</Copyright>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Skybrud.Umbraco.GridData.Dtge</PackageId>
    <PackageTags>Skybrud, Umbraco, Grid</PackageTags>
    <PackageLicenseUrl>https://github.com/skybrud/Skybrud.Umbraco.GridData.Dtge/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/skybrud/Skybrud.Umbraco.GridData.Dtge</PackageProjectUrl>
    <PackageIconUrl>http://www.skybrud.dk/img/5431sk/icon/favicon.ico</PackageIconUrl>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Skybrud.Umbraco.GridData.Dtge.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Skybrud.Umbraco.GridData" Version="2.0.6" />
    <PackageReference Include="Our.Umbraco.DocTypeGridEditor" Version="0.6.1" />
  </ItemGroup>

</Project>